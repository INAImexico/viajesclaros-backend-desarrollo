<!DOCTYPE html>
<html lang="en" ng-app="viajesadmin">
  <head>
    <title>Viajes Claros - Admin</title>
<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!--[if lt IE 9]>
<script src="js/html5shiv.js"></script>
<script src="js/respond.min.js"></script>
<![endif]-->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!--STYLES-->
    <link rel="stylesheet" href="/styles/bootstrap-responsive.min.css">
    <link rel="stylesheet" href="/styles/css/bootstrap.css">
    <link rel="stylesheet" href="/styles/css/bootstrap-theme.min.css">
    <link rel="stylesheet" href="/styles/css/font-awesome.min.css">
    <link rel="stylesheet" href="/styles/general.css">
    <link rel="stylesheet" href="/styles/importer.css">
    <!--STYLES END-->
	
  </head>

<body>
    <div id="mainwrapper" class="mainwrapper">
        <%- partial('global/header') -%> 
        <%- partial('global/menu') -%> 

        <div class="rightpanel">
            <script type='text/javascript'>window.breadcrumb = <%- JSON.stringify(breadcrumb) %>;</script>
            <div breadcrumb></div>        
            
            <div class="pageheader">
                <form action="#" method="post" class="searchbar">
                    <input type="text" name="keyword" placeholder="Escribe para realizar una busqueda" ng-model='searchInput'/>
                </form>
                <div class="pageicon"><span class="<%= page.icon || 'fa fa-th-large' %>"></span></div>
                <div class="pagetitle">
                    <!-- <h5>All Features Summary</h5> -->
                    <h1> <%=page.name || 'Dashboard' %></h1>
                    <p class="page-description"><%= page.description || '' %></p>
                    <!--<p class="page-description">AQUI PODRAS VISUALIZAR Y ADMINISRAR TODO TU PROCESO DE VENTA</p>-->
                </div>
            </div><!--pageheader-->
            
            <div class="maincontent">
                <div class="maincontentinner">
                    <div class="row">
    			     <%- body %>
                    </div><!--row-->
                    
                    <div class="footer">
                        <div class="footer-left">
                            <span>&copy; 2014</span>
                        </div>
                        <div class="footer-right">
                            <span>Desarrollo: <a href="http://spaceshiplabs.com/">Spaceshiplabs.com</a></span>
                        </div>
                    </div><!--footer-->
                    
                </div><!--maincontentinner-->
            </div><!--maincontent-->
            
        </div><!--rightpanel-->
        
    </div><!--mainwrapper-->

        <!--TEMPLATES-->
        <script type="text/javascript" src="/jst.js"></script>
        <!--TEMPLATES END-->

        <!--SCRIPTS-->
        <script src="/js/dependencies/sails.io.js"></script>
        <script src="/bower_components/jquery/dist/jquery.min.js"></script>
        <script src="/bower_components/angular/angular.min.js"></script>
        <script src="/bower_components/angular-ui-bootstrap-bower/ui-bootstrap-tpls.js"></script>
        <script src="/bower_components/ng-file-upload/angular-file-upload.js"></script>
        <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <script src="/bower_components/moment/min/moment-with-locales.min.js"></script>
        <script src="/js/app.js"></script>
        <script src="/js/custom.js"></script>
        <script src="/js/directives/breadcrumb.js"></script>
        <script src="/js/directives/directoryListing.js"></script>
        <script src="/js/directives/fileManager.js"></script>
        <script src="/js/directives/formHelper.js"></script>
        <script src="/js/directives/profileImage.js"></script>
        <script src="/js/directives/tableHelper.js"></script>
        <!--SCRIPTS END-->
        <% if(page.controller){%>
            <script src="/js/controllers/<%- page.controller -%>"></script>
       <% } %>
</body>
</html>
